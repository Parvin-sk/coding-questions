class hello{
    public static int hi(int n){
        int a=0,b=1,result=0;
        if(n==0 || n==1){
            return n;
        }
        for(int i=2;i<=n;i++){
            result=a+b;
            a=b;
            b=result;
            
        }
        return result;
    }
    public static void main(String...args){
        int n=0;
        System.out.println(hi(n));
    }
}
